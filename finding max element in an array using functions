// find the max element in an array using functions
#include <stdio.h>


int main() {
    int hostno=-9999;
    int arr1[10]={78,500,67,12,1000,32,56,13,89,45};
    printf("the max element in array is %d",maxelement(hostno,arr1));

    
}
int maxelement(int hostno,int arr1[]){
    int i;
    for(i=0;i<10;i++){
        if(hostno<arr1[i]){
            hostno=arr1[i];
        }
    }
    return hostno;
}
