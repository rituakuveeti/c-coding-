// program to print first 50 natural numbers using recursion
#include <stdio.h>
int n;

int main() {
   int j,i=1;
   printf("enter the value of n:");
   scanf("%d",&n);
   for(j=0;j<n;j++){
   printf("%d\t",naturalnum(i));
   i++;
   
   }
   
}
int naturalnum(int x){
    if(x!=n)
    return x; 
    else
    return n;
}
